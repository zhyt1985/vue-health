{
  "name": "vue-health",
  "version": "2.0.0",
  "description": "Diagnose and fix performance issues in your Vue app",
  "keywords": [
    "diagnostics",
    "linter",
    "nuxt",
    "performance",
    "vue"
  ],
  "homepage": "https://github.com/zhyt1985/vue-health#readme",
  "bugs": {
    "url": "https://github.com/zhyt1985/vue-health/issues"
  },
  "license": "MIT",
  "author": "zhyt1985",
  "repository": {
    "type": "git",
    "url": "https://github.com/zhyt1985/vue-health.git",
    "directory": "packages/vue-health"
  },
  "bin": {
    "vue-health": "./dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/cli.d.mts",
      "default": "./dist/cli.mjs"
    },
    "./api": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./eslint-plugin": {
      "types": "./dist/eslint-plugin/index.d.mts",
      "default": "./dist/eslint-plugin/index.mjs"
    }
  },
  "scripts": {
    "dev": "tsdown --watch",
    "build": "rm -rf dist && NODE_ENV=production tsdown",
    "typecheck": "tsc --noEmit",
    "test": "pnpm build && vitest run"
  },
  "dependencies": {
    "commander": "^14.0.0",
    "eslint": "^10.0.0",
    "eslint-plugin-vue": "^10.8.0",
    "knip": "^5.83.1",
    "ora": "^9.3.0",
    "oxlint": "^1.47.0",
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2",
    "typescript": ">=5.0.4 <7",
    "vue-eslint-parser": "^10.4.0"
  },
  "devDependencies": {
    "@types/prompts": "^2.4.9",
    "tsdown": "^0.20.0",
    "vitest": "^3.0.0"
  }
}
